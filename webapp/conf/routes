# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# DB Test Operations
GET		/create/one					controllers.Debugging.addRuleOne()
GET		/create/two					controllers.Debugging.addRuleTwo()
GET		/show/constraint			controllers.Debugging.showAllConstraints()
GET		/show/rule					controllers.Debugging.showAllRules()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

